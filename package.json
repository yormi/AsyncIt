{
  "name": "test-them-all",
  "version": "0.1.0",
  "description": "Solution for easy integration/end-to-end test with React",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir dist/src",
    "test": "NODE_ENV=dev mocha test --compilers js:babel-core/register -r babel-polyfill --recursive",
    "test-watch": "nodemon -q --exec 'npm run test -s'",
    "lint": "standard | snazzy",
    "posttest": "npm run lint -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yormi/test-them-all.git"
  },
  "keywords": [
    "React",
    "test",
    "end",
    "integration"
  ],
  "author": "Guillaume Lambert <glamb93@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-project-relative-import": "^1.1.0",
    "eslint": "^2.7.0",
    "nodemon": "^1.9.1",
    "sinon": "^1.17.3",
    "snazzy": "^3.0.0",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "assert": "^1.3.0",
    "babel-polyfill": "^6.7.4",
    "jsdom": "^8.3.0",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-component-errors": "git+https://github.com/staxmanade/react-component-errors.git",
    "react-dom": "^0.14.8",
    "unexpected": "^10.13.0",
    "unexpected-react": "^2.0.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": []
  }
}
